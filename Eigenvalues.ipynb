# In[15]:


import sympy as sp
from IPython.display import display, Math

# Define symbols for matrix elements and eigenvalues
H11, H12, H21, H22, λ = sp.symbols('H11 H12 H21 H22 λ', complex=True)

# Ensure Hermitian property (H21 = H12*)
H = H.subs(H21, sp.conjugate(H12))

# Define the Hermitian matrix
H = sp.Matrix([[H11, H12], [H21, H22]])

# Compute trace and determinant
trace_H = sp.trace(H)  # Tr(H)
det_H = H.det()        # det(H)


# In[19]:


# Part (a): Find the eigenvalues
eigenvalues = H.eigenvals()
print("Eigenvalues:")
for eig in eigenvalues:
    display(sp.simplify(eig))


# In[11]:


# Part (b): Find the eigenstates
eigenvectors = H.eigenvects()
print("\nEigenstates:")
for eigval, multiplicity, eigvecs in eigenvectors:
    print(f"Eigenvalue: ")
    display(sp.simplify(eigval))
    for vec in eigvecs:
        display(sp.simplify(vec))


